<p id="notice"><%= notice %></p>

<h1>Listing Sale Productos</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Venta</th>
      <th>Producto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sale_productos.each do |sale_producto| %>
      <tr>
        <td><%= sale_producto.sale.folio %></td>
        <td><%= sale_producto.producto.nombre %></td>
        <td><%= link_to 'Show', sale_producto %></td>
        <td><%= link_to 'Edit', edit_sale_producto_path(sale_producto) %></td>
        <td><%= link_to 'Destroy', sale_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sale producto', new_sale_producto_path %>
